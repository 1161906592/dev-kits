<%- include( `${process.cwd()}/.swagger/formFieldsHelper.ejs`) %>

// 表单字段
const renderFields = () => {
  const model = modelRef.value
  return (
    <>
      <% fields.filter((d) => d.key !== 'id').forEach(function(item){ %>
        <NFormItem label="<%- item.title %>:" path="<%- item.key %>">
          <%- include( `${process.cwd()}/.swagger/formItem.ejs`, { item }) %>
        </NFormItem>
      <% }); %>
    </>
  )
}